{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": [
        "wait"
      ]
    },
    "next-redux-wrapper#start": {
      "cache": false,
      "persistent": true,
      "dependsOn": []
    },
    "wait": {
      "cache": false,
      "dependsOn": [
        "^wait"
      ]
    },
    "build": {
      "dependsOn": [
        "^build"
      ]
    },
    "next-redux-wrapper#build": {
      "outputs": [
        "packages/*/lib/**",
        "packages/*/es6/**",
        "packages/*/.next/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    },
    "clean": {
      "dependsOn": [
        "^clean"
      ],
      "cache": false
    },
    "publish:release": {
      "dependsOn": [
        "build"
      ],
      "cache": false
    }
  }
}